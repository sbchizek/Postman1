		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"const jsonData = pm.response.body != null ? pm.response.json() : null;",
					"",
					"pm.test('Status code is 200', function () {",
					"    pm.response.to.have.status(200);",
					"});",
					"",
					"pm.test('No Errors', function () {",
					"    if(jsonData != null) {",
					"        console.debug('JSON data returned, verifying no  \"message\" property');",
					"        pm.expect(jsonData).to.not.have.property('message');",
					"    } else {",
					"        console.debug('No JSON data returned, skipping error message check');",
					"    }",
					"});"
				]
			}
		}
	],
